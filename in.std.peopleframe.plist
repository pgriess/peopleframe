<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>in.std.peopleframe</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/poetry</string>
        <string>run</string>
        <string>peopleframe</string>
        <string>-vvv</string>
        <string>-f</string>
        <string>/Users/pg/.peopleframe.ini</string>
        <string>-n</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/pg/src/pgriess-peopleframe</string>
    <key>StandardErrorPath</key>
    <string>/Users/pg/Library/Logs/peopleframe.err</string>
    <key>StandardOutPath</key>
    <string>/Users/pg/Library/Logs/peopleframe.out</string>
    <key>WatchPaths</key>
    <array>
        <string>/Users/pg/pfwatch</string>
    </array>
</dict>
</plist>
